{
    "compilerOptions": {
        "outDir": "dist",
        "noImplicitAny": true,
        "module": "CommonJS",
        "target": "es5",
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "sourceMap": true
    },
    "compileOnSave": false,
    "exclude": ["node_modules", "dist"]
}
